/// @brief Updated validity check
bool doValid() const
{
    if (!Base::doValid()) {
        return false;
    }
    
    if ((field_flags().field_high().getBitValue_passwordFlag()) &&
        (!field_flags().field_high().getBitValue_userNameFlag())) {
        return false;
    }

    return 
        (Base::transportField_flags().field_retain().value() == 0U) &&
        (static_cast<unsigned>(Base::transportField_flags().field_qos().value()) == 0U) &&
        (Base::transportField_flags().field_dup().value() == 0U);
}
